  <Kpis>
    <Kpi dwd:design-time-name="8b173635-4ad2-43ac-a949-640da9afe7e0">
      <ID>KPI</ID>
      <Name>Qty KPI</Name>
      <Value>[Measures].[Qty]</Value>
      <Goal>20</Goal>
      <Status>case
    when kpivalue("Qty KPI") &gt; kpigoal("Qty KPI") then 1
    when kpivalue("Qty KPI") &lt; kpigoal("Qty KPI") then -1
    when kpivalue("Qty KPI") = kpigoal("Qty KPI") then 0
end</Status>
      <Trend></Trend>
      <TrendGraphic>Standard Arrow</TrendGraphic>
      <StatusGraphic>Gauge - Ascending</StatusGraphic>
    </Kpi>
  </Kpis>
